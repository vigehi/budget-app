<div class="myclass">
  <%= link_to "Back", groups_path, class:"back-link" %>
  <p>ADD TRANSACTION</p>
  <div >
  <%= link_to "Log Out", destroy_user_session_path,  class: "c-actions" %> 
  </div>
</div>

<% flash.each do |type, msg| %>
    <%= content_tag :div, msg, class: "#{type} alert" %>
    <% end %>
    <%= form_with url: new_group_transaction_path, scope: :transaction do |f| %>
          <%= f.text_field :name, placeholder: 'Name', class:"field-login" %>
          <%= f.text_field :amount, placeholder: 'Amount', class:"field-login" %>
          <%= f.submit "submit", class:"btn btn-primary my-btn2" %>
        <% end %> 